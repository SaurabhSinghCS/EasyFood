</head>

<body>
    <div id="id01" class="modal">
        <div class="boxlog">
            <h1>Login</h1>
            <form method="POST" action="/admin_login" class="animate" id="myform">
                <div class="inputbox">
                    <input type="text" name="username" required>
                    <label> username </label>
                </div>
                <br>
                <div class="inputbox">
                    <input type="password" name="password" required><br><br>
                    <label>password</label>
                </div>
                <button type="submit" id="rege">Log in</button>
                <br><br>
                <h5 style="color: white;">Don't have an account &nbsp;&nbsp;&nbsp;&nbsp;<a style="position: absolute;color: blue;" hover
                        onclick="document.getElementById('id02').style.display='block' , document.getElementById('id01').style.display='none'">
                        &nbsp;Sign up</a></h5>
                <div class="container1" style="background-color: transparent">
                    <button type="button" onclick="document.getElementById('id01').style.display='none'"
                        class="cancelbtn">Cancel</button>
                </div>
            </form>
        </div>
    </div>

    <div id="id02" class="modal">
        <div class="boxlog">
            <h2 id="reg1">Register Yourself</h2>
            <form method="POST" action="/admin_register" class="animate" id="myform1">
                <div class="inputbox">
                    <input type="text" name="name" required>
                    <label>Enter Your Name</label>
                </div>
                <br>
                <div class="inputbox">
                    <input type="email" name="email" required>
                    <label>Email</label>
                </div>
                <br>
                <div class="inputbox">
                    <input type="text" name="mob_number" maxlength="10" minlength="10" required>
                    <label>Mobile Number</label>
                </div>
                <br>
                <div class="inputbox">
                    <input type="username" name="username" required>
                    <label>Username</label>
                </div><br>
                <div class="inputbox">
                    <input type="password" name="password" required>
                    <label>Enter Password</label>
                </div>
                <br>
                <button type="submit"
                    style="letter-spacing: 2px; font-size: 19px; margin-top: 15px; margin-left: 95px;">Register</button>
                <br>
                <h5 style="color: white;">Already have an account &nbsp;&nbsp;&nbsp;&nbsp;<a style="position: absolute;"
                        onclick="document.getElementById('id02').style.display='none' , document.getElementById('id01').style.display='block'">
                        &nbsp;Log in</a></h5>
                <div class="container1" style="background-color: transparent">
                    <button type="button" onclick="document.getElementById('id01').style.display='none'"
                        class="cancelbtn">Cancel</button>
                </div>
            </form>
        </div>
    </div>
    <div id="to_be_viewed" style="display: none;" value="<%= a %>"></div>
    <script>
        if(document.getElementById('to_be_viewed').value == 'login'){
            document.getElementById('id01').style.display='block'
        }
        else{
            document.getElementById('id02').style.display='block'
        }
    </script>
</body>

