        @keyframes rotateWordsFirst {
            0% {
                font-size: 5px;
                opacity: 0;
                width: 0px;
            }

            5% {
                font-size: 25px;
                opacity: 0.1;
                width: 50%;
            }

            17% {
                font-size: 35px;
                opacity: 0.4;
                width: 100%;
            }

            20% {
                font-size: 50px;
                opacity: 0.4;
            }

            50% {
                font-size: 63px;
                opacity: .5;
            }

            100% {
                opacity: 1;
            }
        }

        @keyframes rotateWordsSecond {

            0% {
                opacity: 0;
                transform: translateX(-100%) rotate(-240deg);
            }

            50% {
                opacity: 0.5;
                transform: translateX(-50%) rotate(-120deg);
            }

            100% {
                opacity: 1;
                transform: translateX(0px) rotate(0deg);
            }
